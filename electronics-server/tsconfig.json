{
	"ts-node": {
		"files": true
	},
	"compilerOptions": {
		"types": [
			"jest", "node"
		],
		"module": "CommonJS",
		"target": "ES5",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,

	}
}